{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "Gateway": {
    "Cors": {
      "AllowedOrigins": [
        "https://localhost:5001",
        "https://localhost:5101"
      ]
    },
    "Auth": {
      "AnonymousPathPrefixes": [
        "/api/auth",
        "/health"
      ]
    },
    "RateLimiting": {
      "PermitLimit": 200,
      "WindowSeconds": 60
    }
  },
  "ReverseProxy": {
    "Routes": {
      "route_api": {
        "ClusterId": "api",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      },
      "route_hubs": {
        "ClusterId": "api",
        "Match": {
          "Path": "/hubs/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "api": {
        "Destinations": {
          "api_1": {
            "Address": "https://localhost:5101/"
          }
        }
      }
    }
  }
}