{
  "ReverseProxy": {
    "Routes": {
      "route_api": {
        "ClusterId": "api",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      },
      "route_hubs": {
        "ClusterId": "api",
        "Match": {
          "Path": "/hubs/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "api": {
        "Destinations": {
          "api_1": {
            "Address": "https://localhost:5101/"
          }
        }
      }
    }
  }
}
